{
  "name": "@typeform/embed",
  "version": "0.34.5",
  "description": "Library to embed typeform in your website",
  "author": "Typeform",
  "license": "LGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/Typeform/embed.git"
  },
  "main": "build/lib.pure.js",
  "module": "build/lib.pure.js",
  "files": [
    "assets",
    "build/lib.pure.js",
    "build/lib.pure.js.map",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf build dist",
    "build": "./scripts/build.sh",
    "serve-demo": "PORT=8080 yarn serve -S demo",
    "start": "NODE_ENV=development webpack-dev-server -d --config webpack.config.js --content-base ./demo",
    "test": "yarn lint && yarn test:unit && yarn test:functional && yarn test:visual",
    "lint": "yarn eslint ./src --ext .js --ignore-path .eslintignore --max-warnings=0 && prettier --check . --ignore-path .eslintignore",
    "prettier": "prettier --write . --ignore-path .eslintignore",
    "test:unit": "yarn jest",
    "test:unit:watch": "yarn jest --watch",
    "test:unit:coverage": "yarn jest --coverage",
    "ci:functional": "start-server-and-test serve-demo 8080 test:functional",
    "ci:visual": "start-server-and-test serve-demo 8080 test:visual",
    "test:visual": "yarn cypress run --spec e2e/spec/visual/**/* --browser chrome",
    "test:functional": "yarn cypress run --spec e2e/spec/functional/**/* --browser chrome",
    "test:debug": "yarn cypress open",
    "travis-deploy-once": "yarn travis-deploy-once --pro"
  },
  "devDependencies": {
    "@applitools/eyes-cypress": "^3.18.7",
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@typeform/eslint-config": "^3.0.2",
    "@wdio/cli": "^6.12.1",
    "@wdio/selenium-standalone-service": "^6.12.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "core-js": "^3.8.3",
    "css-loader": "^3.6.0",
    "cypress": "^6.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.19.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "preact": "^10.5.12",
    "prettier": "^2.2.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "regenerator-runtime": "^0.13.7",
    "selenium-standalone": "^6.23.0",
    "semantic-release": "^17.3.7",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.12.0",
    "style-loader": "^1.2.1",
    "travis-deploy-once": "^5.0.11",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "custom-event": "^1.0.1",
    "is-url": "^1.2.2",
    "prop-types": "^15.7.2",
    "scrollbar-width": "^3.1.1",
    "spin.js": "^4.1.0",
    "styled-components": "^5.2.1",
    "url-parse": "^1.4.5"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  }
}
