{
  "name": "@typeform/embed",
  "version": "0.24.2",
  "description": "Library to embed Typeforms in your website",
  "author": "Typeform",
  "license": "LGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/Typeform/embed.git"
  },
  "main": "build/lib.pure.js",
  "module": "build/lib.pure.js",
  "files": [
    "assets",
    "build/lib.pure.js",
    "build/lib.pure.js.map",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf build dist",
    "build": "./scripts/build.sh",
    "serve-demo": "PORT=8080 yarn serve -S demo",
    "start": "NODE_ENV=development webpack-dev-server -d --config webpack.config.js --content-base ./demo",
    "test": "yarn lint && yarn test:unit && yarn test:functional && yarn test:visual",
    "lint": "yarn eslint ./src --ext .js --ignore-path .eslintignore --max-warnings=0",
    "test:unit": "yarn jest",
    "test:unit:watch": "yarn jest --watch",
    "test:unit:coverage": "yarn jest --coverage",
    "test:visual": "yarn run test:visual:chrome && yarn run test:visual:firefox",
    "test:visual:firefox": "yarn run test:visual:run --browser firefox",
    "test:visual:chrome": "yarn run test:visual:run --browser chrome",
    "test:visual:run": "yarn cypress run --spec e2e/spec/visual/**/*",
    "test:functional": "yarn run test:functional:chrome && yarn run test:functional:firefox",
    "test:functional:firefox": "yarn run test:functional:run --browser firefox",
    "test:functional:chrome": "yarn run test:functional:run --browser chrome",
    "test:functional:run": "yarn cypress run --spec e2e/spec/functional/**/*",
    "test:debug": "yarn cypress open",
    "prepublish": "yarn run build",
    "travis-deploy-once": "yarn travis-deploy-once --pro"
  },
  "devDependencies": {
    "@applitools/eyes-cypress": "^3.12.0",
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@typeform/eslint-config": "^1.12.0",
    "@wdio/cli": "^6.1.17",
    "@wdio/selenium-standalone-service": "^6.1.14",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "cypress": "^4.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.1",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "preact": "^10.4.4",
    "regenerator-runtime": "^0.13.5",
    "selenium-standalone": "^6.17.0",
    "semantic-release": "^17.0.8",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1",
    "travis-deploy-once": "^5.0.11",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "custom-event": "^1.0.1",
    "is-url": "^1.2.2",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "scrollbar-width": "^3.1.1",
    "spin.js": "^4.1.0",
    "styled-components": "^5.1.1",
    "url-parse": "^1.4.5"
  }
}
