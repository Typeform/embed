{
  "name": "@typeform/embed",
  "version": "1.0.0-next.1",
  "private": true,
  "main": "build/index.js",
  "browser": "build/embed-next.js",
  "author": "Typeform",
  "license": "LGPL-3.0-only",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "webpack",
    "copy:lib": "mkdir -p ../embed-kitchen-sink/public/ && cp ./build/embed-next.js ../embed-kitchen-sink/public/",
    "dev": "tsc -w",
    "demo": "yarn build && yarn copy:lib && cd ../embed-kitchen-sink && yarn dev",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx --max-warnings=0 && yarn prettier-check",
    "prettier-check": "prettier --check . --ignore-path .eslintignore",
    "prettier": "prettier --write . --ignore-path .eslintignore",
    "test": "jest",
    "cy:functional": "yarn cypress run --spec e2e/spec/functional/**/* --browser chrome --headless",
    "cy:open": "yarn cypress open",
    "cy:visual": "yarn cypress run --spec e2e/spec/visual/**/* --browser chrome",
    "test:functional": "start-server-and-test demo 6006 cy:functional",
    "test:visual": "start-server-and-test demo 6006 cy:visual"
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.0.0",
    "ts-loader": "^8.0.15",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  }
}
